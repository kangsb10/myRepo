package kr.or.ddit.vo;

import java.util.List;


public class CultivationMethodVO {
	
	/**
	 *  재배 방법 VO
	 *  ctvt_mth_no = 재배방법 글 번호 
	 *	mber_ncnm = 재배방법 글 작성자
	 *	ctvt_mth_sj = 재배방법 글 제목
	 * 	ctvt_mth_cn = 재배방법 글 내용
	 *	ctvt_mth_input_de = 재배방법 글 입력날짜
	 *  
	 */
	
	private String rnum;
	private String ctvt_mth_no;  
	private String mber_ncnm;
	private String ctvt_mth_sj;
	private String ctvt_mth_cn;
	private String ctvt_mth_input_de;
	private String ctvt_mth_hit;
	private String ctvt_mth_delete;
	private List<CultivationMethodFileItemVO> items;
	
	
	public String getRnum() {
		return rnum;
	}
	public void setRnum(String rnum) {
		this.rnum = rnum;
	}
	public String getCtvt_mth_no() {
		return ctvt_mth_no;
	}
	public void setCtvt_mth_no(String ctvt_mth_no) {
		this.ctvt_mth_no = ctvt_mth_no;
	}
	public String getMber_ncnm() {
		return mber_ncnm;
	}
	public void setMber_ncnm(String mber_ncnm) {
		this.mber_ncnm = mber_ncnm;
	}
	public String getCtvt_mth_sj() {
		return ctvt_mth_sj;
	}
	public void setCtvt_mth_sj(String ctvt_mth_sj) {
		this.ctvt_mth_sj = ctvt_mth_sj;
	}
	public String getCtvt_mth_cn() {
		return ctvt_mth_cn;
	}
	public void setCtvt_mth_cn(String ctvt_mth_cn) {
		this.ctvt_mth_cn = ctvt_mth_cn;
	}
	public String getCtvt_mth_input_de() {
		return ctvt_mth_input_de;
	}
	public void setCtvt_mth_input_de(String ctvt_mth_input_de) {
		this.ctvt_mth_input_de = ctvt_mth_input_de;
	}
	public String getCtvt_mth_hit() {
		return ctvt_mth_hit;
	}
	public void setCtvt_mth_hit(String ctvt_mth_hit) {
		this.ctvt_mth_hit = ctvt_mth_hit;
	}
	public List<CultivationMethodFileItemVO> getItems() {
		return items;
	}
	public void setItems(List<CultivationMethodFileItemVO> items) {
		this.items = items;
	}
	public String getCtvt_mth_delete() {
		return ctvt_mth_delete;
	}
	public void setCtvt_mth_delete(String ctvt_mth_delete) {
		this.ctvt_mth_delete = ctvt_mth_delete;
	}
	
	
}
