        <![endif]-->

<!-- Favicon and touch icons -->
<link rel="shortcut icon" href="/css/shop/assets/ico/favicon.png">
<link rel="apple-touch-icon-precomposed" sizes="144x144"
	href="/css/shop/assets/ico/apple-touch-icon-144-precomposed.png">
