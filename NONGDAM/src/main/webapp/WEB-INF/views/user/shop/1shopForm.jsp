</head>

<body>

	<!-- Top menu -->
	<nav class="navbar navbar-inverse navbar-no-bg" role="navigation">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed"
					data-toggle="collapse" data-target="#top-navbar-1">
					<span class="sr-only">Toggle navigation</span> <span
						class="icon-bar"></span> <span class="icon-bar"></span> <span
						class="icon-bar"></span>
				</button>
				<a class="navbar-brand" href="index.html">Bootstrap Registration
					Form Template</a>
			</div>
			<!-- Collect the nav links, forms, and other content for toggling -->
			<div class="collapse navbar-collapse" id="top-navbar-1">
				<ul class="nav navbar-nav navbar-right">
					<li><span class="li-text"> Put some text or </span> <a
						href="#"><strong>links</strong></a> <span class="li-text">
							here, or some icons: </span> <span class="li-social"> <a href="#"><i
								class="fa fa-facebook"></i></a> <a href="#"><i
								class="fa fa-twitter"></i></a> <a href="#"><i
								class="fa fa-envelope"></i></a> <a href="#"><i
								class="fa fa-skype"></i></a>
					</span></li>
				</ul>
			</div>
		</div>
	</nav>

	<!-- Top content -->
	<div class="top-content">

		<div class="inner-bg">
			<div class="container">
				<div class="row">
					<div class="col-sm-8 col-sm-offset-2 text">
						<h1>
							<strong>Bootstrap</strong> Registration Form
						</h1>
						<div class="description">
							<p>
								This is a free responsive registration form made with Bootstrap.
								Download it on <a href="http://azmind.com"><strong>AZMIND</strong></a>,
								customize and use it as you like!
							</p>
						</div>
					</div>
				</div>
				<div class="row">
					<!--                     	<div class="col-sm-6 book"> -->
					<!--                     		<img src="/css/shop/assets/img/ebook.png" alt=""> -->
					<!--                     	</div> -->
					<div class="col-sm-5 form-box">
						<div class="form-top">
							<div class="form-top-left">
								<h3>상품을 등록하세요</h3>
								<!--                             		<p>Fill in the form below to get instant access:</p> -->
							</div>
							<div class="form-top-right">
								<i class="fa fa-pencil"></i>
							</div>
						</div>
						<div class="form-bottom">
							<form role="form" action="" method="post"
								class="registration-form">
								<div class="form-group">
									<label class="sr-only" for="goods_id">상품코드</label>
									<div class="col-sm-10">
										<input type="text" class="form-control" id="goods_id" placeholder="상품코드">
									</div>
									<button
										class=""
										id="search_goods_id" type="button">상품코드검색</button>
								</div>
								<div class="form-group">
									<label class="sr-only" for="form-last-name">상품명</label> 
									<input type="text" placeholder="상품명"
										class="form-last-name form-control" id="goods_nm">
								</div>
								<div class="form-group">
									<label class="sr-only" for="form-last-name">상품분류코드</label> 
											<select class="browser-default" name="lprod_gu" id="lprod_gu">
<%-- 											    <%for(String lprodStr : listGuPlusMm){ %> --%>
<%-- 												<option value="<%=lprodStr.substring(0,4)%>" ><%=lprodStr%></option> --%>
<%-- 												<%} %> --%>
											</select>
								</div>
								<div class="form-group">
									<label class="sr-only" for="buyer_id">거래처코드</label>
									<div class="col-sm-10">
										<input type="text" class="" id="buyer_id" placeholder="거래처코드">
									</div>
									<button
										class=""
										id="search_buyer_id" type="button">거래처코드검색</button>
								</div>
								<div class="form-group">
									<label class="sr-only" for="goods_pc">상품 가격</label> <input
										type="text" name="form-email" placeholder="가격을 원단위로 입력하세요"
										class="form-email form-control" id="goods_pc">
								</div>
								<div class="form-group">
									<label class="sr-only" for="goods_invntry">재고량</label> <input
										type="text" name="goods_invntry" placeholder="재고수량을 입력하세요"
										class="form-email form-control" id="goods_invntry">
								</div>
								<div class="form-group">
									<label class="sr-only" for="goods_dc">상품 설명</label> <input
										type="text" name="goods_dc" placeholder="상품 상세설명"
										class="form-email form-control" id="goods_dc">
								</div>
								<div class="form-group">
								<label class="" for="file01">상품이미지</label>
								<div class="">
									 <input type="file" class="filestyle" id="files" name="files" data-buttonName="btn-primary">
								</div>
							</div>
							<div class="form-group">
								<label class="" for="file02">첨부이미지</label>
								<div class="">
									 <input type="file" class="filestyle" id="files" name="files" data-buttonName="btn-primary">
								</div>
							</div>
							<div class="form-group"> 
								<div class="">
									<button type="submit" class="" style="float: right">등록</button>
									<button type="button" class="" style="float: right" id="btn_cncl">취소</button>
									<button type="button" class="" style="float: right" id="btn_list">목록</button>
								</div>
							</div>
<!-- 								<button type="submit" class="btn">등록하기</button> -->
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>

	</div>


	<!-- Javascript -->
	<script src="/css/shop/assets/js/jquery-1.11.1.min.js"></script>
	<script src="/css/shop/assets/bootstrap/js/bootstrap.min.js"></script>
	<script src="/css/shop/assets/js/jquery.backstretch.min.js"></script>
	<script src="/css/shop/assets/js/retina-1.1.0.min.js"></script>
	<script src="/css/shop/assets/js/scripts.js"></script>

	<!--[if lt IE 10]>
            <script src="assets/js/placeholder.js"></script>
        <![endif]-->

</body>
<script type="text/javascript" src="http://code.jquery.com/jquery-latest.js"></script>
